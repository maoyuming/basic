<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="otsTables" targetRuntime="MyBatis3">
	
		<plugin type="com.duantuke.basic.util.mybatis.PaginationPlugin"></plugin>

		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://211.157.155.198:3306/otc"
			userId="root" password="dtk@2016">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		
		<javaModelGenerator targetPackage="com.duantuke.basic.po" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
		<sqlMapGenerator targetPackage="mappers" targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER" targetPackage="com.duantuke.basic.mappers" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
        
<!--          <table tableName="meal" domainObjectName="Meal"></table>  -->
         <table tableName="skus" domainObjectName="Sku"></table> 
<!--         <table tableName="room_type" domainObjectName="RoomType"></table> -->
	</context>
</generatorConfiguration>